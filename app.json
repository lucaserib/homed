{
  "expo": {
    "name": "homed",
    "slug": "homed",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F80ED"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lucaserib.homed",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "O Homed precisa de acesso à sua localização para encontrar médicos próximos e direcionar o atendimento até você.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "O Homed precisa de acesso à sua localização para encontrar médicos próximos e direcionar o atendimento até você."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDikOrJbDtBSxOHvNbbRnikcdIhHKm3tbs"
        }
      },
      "package": "com.lucaserib.homed"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://uber.com/"
        }
      ],
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "O Homed precisa de acesso à sua localização para encontrar médicos próximos e direcionar o atendimento até você."
        }
      ]
    ]
  }
}
